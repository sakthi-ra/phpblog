<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>load-test</goal>
        <description>Goal which load Test to the PHP project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
            <dependency>
                <groupId>com.photon.phresco.plugins</groupId>
                <artifactId>php-phresco-plugin</artifactId>
                <type>jar</type>
                <version>2.0.0.35001</version>
                <class>com.photon.phresco.plugins.php.PHPPlugin</class>
            </dependency>
        </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Test Against</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>testAgainst</key>
                    <possibleValues>
                        <value key="server">Server</value>
                        <value key="webservice">Web Services</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>server</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>configurations</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.commons</groupId>
<artifactId>phresco-commons</artifactId>
<type>jar</type>
<version>2.0.0.35001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>false</multiple>
                    <value>Production</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>configurations</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Configurations</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.PerformanceConfigurationsImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.framework</groupId>
<artifactId>phresco-framework-impl</artifactId>
<type>jar</type>
<version>2.0.0.35001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>configurations</key>
                    <multiple>false</multiple>
                    <value>server</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Test Result Name</value>
                    </name>
                    <type>String</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>testName</key>
                    <multiple>false</multiple>
                    <value>test</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">NoOfUsers</value>
                    </name>
                    <type>Number</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>noOfUsers</key>
                    <multiple>false</multiple>
                    <value>10</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Ramp-Up Period</value>
                    </name>
                    <type>Number</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>rampUpPeriod</key>
                    <multiple>false</multiple>
                    <value>1</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Loop Count</value>
                    </name>
                    <type>Number</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>loopCount</key>
                    <multiple>false</multiple>
                    <value>10</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Headers</value>
                    </name>
                    <type>map</type>
                    <childs>
                        <child>
                            <name>
<value lang="en">Key</value>
                            </name>
                            <type>String</type>
                            <key>headerKey</key>
                            <required>true</required>
                            <description>add header key</description>
                        </child>
                        <child>
                            <name>
<value lang="en">Value</value>
                            </name>
                            <type>String</type>
                            <key>headerValue</key>
                            <required>true</required>
                            <description>header value</description>
                        </child>
                    </childs>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>addHeader</key>
                    <multiple>false</multiple>
                    <value>#&#xD;
#Fri Feb 01 13:56:55 IST 2013&#xD;
test=123&#xD;
</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>
